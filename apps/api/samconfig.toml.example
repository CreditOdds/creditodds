# Copy this file to samconfig.toml and update with your values
# For deployment, use ./deploy.sh which reads credentials from .env
version = 0.1
[default]
[default.deploy]
[default.deploy.parameters]
stack_name = "CreditCardOddsAPI"
s3_bucket = "your-s3-bucket-name"
s3_prefix = "CreditCardOddsAPI"
region = "us-east-2"
capabilities = "CAPABILITY_IAM"
parameter_overrides = ""
image_repositories = []
confirm_changeset = false
disable_rollback = true
